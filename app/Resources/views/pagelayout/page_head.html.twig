    <meta name="msapplication-square150x150logo" content="{{ asset('assets/public/images/mstile-150x150.png') }}" />
    <meta name="msapplication-wide310x150logo" content="{{ asset('assets/public/images/mstile-310x150.png') }}" />
    <meta name="msapplication-square310x310logo" content="{{ asset('assets/public/images/mstile-310x310.png') }}" />
    <meta name="author" content="Lucas SIMONIN">
